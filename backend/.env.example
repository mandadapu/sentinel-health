# GCP
GCP_PROJECT_ID=sentinel-health-dev
ENV=dev

# Sidecar
SIDECAR_URL=http://localhost:8081

# Anthropic
ANTHROPIC_API_KEY=

# Firestore
FIRESTORE_COLLECTION=triage_sessions

# Cloud SQL / RAG
CLOUDSQL_INSTANCE=
CLOUDSQL_DSN=  # e.g. postgresql://sentinel:localdev@localhost:5432/sentinel_health
RAG_TOP_K=5
EMBEDDING_MODEL=voyage-3

# Model configuration
DEFAULT_CLASSIFIER_MODEL=claude-haiku-4-5-20241022
SENTINEL_MODEL=claude-haiku-4-5-20241022

# Sentinel thresholds
HALLUCINATION_THRESHOLD=0.15
CONFIDENCE_THRESHOLD=0.85

# Routing
MIN_ROUTING_CONFIDENCE=0.70

# Emulators (for local dev)
PUBSUB_EMULATOR_HOST=localhost:8085
FIRESTORE_EMULATOR_HOST=localhost:8086
